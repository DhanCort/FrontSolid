    <!-- Add Customer Modal -->
    <div class="modal fade" id="addCustomerModal">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">New Customer</h4>
                    <button type="button" class="close btnCloseaddCustomerModal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body autoHeight">
                    <form id="frmAddCustomer">
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-3 text-right" for="strFirstName">First Name:</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="strFirstName" name="strFirstName" required="required">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-3 text-right" for="strLastName">Last Name:</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="strLastName" name="strLastName">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-3 text-right" for="strEmail">Email:</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="strEmail" name="strEmail" required="required">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-sm-3 text-right" for="strPassword">Password:</label>
                                <div class="col-sm-9">
                                    <input type="password" class="form-control" id="strPassword" name="strPassword" required="required">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    <button class="btn btn-primary" type="submit">Save</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger btnCloseaddCustomerModal">Close</button>
                </div>

            </div>
        </div>
    </div>

