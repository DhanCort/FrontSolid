@model List<AttributeModel>
<div class="panel panel-primary">
    <div class="panel-heading">
        <h3>Attributes</h3>
    </div>
    <div class="panel-body">
        <table class="table table-hover">
            <tr>
                <th>CustomName</th>
                <th>Cardinality</th>
                <th>Datatype</th>
                <th>Description</th>
                <th></th>
            </tr>
            @if (
            (List<AttributeModel>)ViewBag.darrattr != null
            )
            {
                foreach (AttributeModel attr in (List<AttributeModel>)ViewBag.darrattr)
                {
                    <tr>
                        <td>
                            @(attr.strCustomName)
                        </td>
                        <td>
                            @(attr.strCardinality)
                        </td>
                        <td>
                            @(attr.strDatatype)
                        </td>
                        <td>
                            @(attr.strDescription)
                        </td>
                        <td>
                            @Html.ActionLink("Add Calculation", "AddCalculation", "Calculation", new { intProductPk = ViewBag.intProductPk, intAttributePk = attr.intnPk }, null)
                        </td>
                    </tr>
                }
            }
        </table>

        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
    </div>
</div>